    "dev:nuxt": "nuxt dev",
    "build": "nuxt build",
    "start": "concurrently \"NODE_ENV=production node server/socket-server.js\" \"nuxt start\"",
    "start:socket": "NODE_ENV=production node server/socket-server.js",
    "start:nuxt": "nuxt start",
    "preview": "nuxt preview",
